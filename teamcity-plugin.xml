<?xml version="1.0" encoding="UTF-8"?>
<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:shemas-jetbrains-com:teamcity-plugin-v1-xml">
  <info>
    <name>teamcity-tests-watchdog</name>
    <display-name>Tests Duration Watchdog</display-name>
    <version>@Version@</version>
    <description>Provides a build failure condition to fail a build on test duration increase</description>
    <download-url>Plugin download URL</download-url>
    <vendor>
      <name>JetBrains</name>
      <url>http://www.jetbrains.com</url>
    </vendor>
  </info>
  <deployment use-separate-classloader="true" node-responsibilities-aware="true"/>
</teamcity-plugin>
